{
    "name": "@jigenlab/ui",
    "version": "0.0.1",
    "description": "React components for user interface from Gigen Lab.",
    "repository": "git@github.com:cocoloring/gigenlab.git",
    "keywords": [
        "typescript"
    ],
    "author": "cocoloring <cocoloring@outlook.com>",
    "license": "MIT",
    "main": "build/main.js",
    "types": "build/main.d.ts",
    "scripts": {
        "build": "pnpm run-s build:*",
        "build:ts": "pnpm tsc -p ./tsconfig.json",
        "dev": "pnpm run-p dev:*",
        "dev:ts": "pnpm tsc -p ./tsconfig.json -w",
        "clean": "pnpm run-p clean:*",
        "clean:node": "pnpm rimraf node_modules",
        "clean:build": "pnpm rimraf build tsconfig.tsbuildinfo"
    },
    "devDependencies": {
        "@types/node": "^16.11.10",
        "@types/react": "^17.0.37",
        "typescript": "^4.5.2"
    },
    "pearDependencies": {
        "react": "17.0.2"
    }
}